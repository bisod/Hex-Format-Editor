{
  "name": "hex-format-editor",
  "displayName": "%name%",
  "description": "%description%",
  "version": "0.0.1",
  "aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
  "publisher": "DuHaofan",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "main": "./dist/extension.js",
  "browser": "./dist/web/extension.js",
  "l10n": "./l10n",
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    },
    "virtualWorkspaces": true
  },
  "contributes": {
    "configuration": [
      {
        "title": "%name%",
        "properties": {
          "hexformateditor.inspectorType": {
            "type": "string",
            "enum": [
              "aside",
              "hover",
              "sidebar"
            ],
            "default": "hover",
            "description": "%hexformateditor.inspectorType%"
          },
          "hexformateditor.maxFileSize": {
            "type": "number",
            "minimum": 0,
            "default": 10,
            "description": "%hexformateditor.maxFileSize%"
          },
          "hexformateditor.dataInspector.autoReveal": {
            "type": "boolean",
            "default": true,
            "description": "%hexformateditor.dataInspector.autoReveal%"
          },
          "hexformateditor.defaultEndianness": {
            "type": "string",
            "default": "little",
            "enum": [
              "little",
              "big"
            ],
            "description": "%hexformateditor.defaultEndianness%"
          },
          "hexformateditor.columnWidth": {
            "type": "integer",
            "default": 16,
            "minimum": 1,
            "maximum": 512,
            "description": "%hexformateditor.columnWidth%"
          },
          "hexformateditor.showDecodedText": {
            "type": "boolean",
            "default": true,
            "description": "%hexformateditor.showDecodedText%"
          },
          "hexformateditor.showOpenFileButton": {
            "type": "boolean",
            "default": false,
            "description": "%hexformateditor.showOpenFileButton%"
          }
        }
      }
    ],
    "customEditors": [
      {
        "viewType": "formatEditor.hexedit",
        "displayName": "%name%",
        "selector": [
          {
            "filenamePattern": "*"
          }
        ],
        "priority": "option"
      }
    ],
    "commands": [
      {
        "command": "formatEditor.openFile",
        "category": "%name%",
        "title": "%hexEditor.openFile%",
        "icon": "$(file-binary)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "hexExplorer",
          "title": "%name%",
          "icon": "panel-icon.svg",
          "when": "hexEditor:showSidebarInspector"
        }
      ]
    },
    "views": {
      "hexExplorer": [
        {
          "type": "webview",
          "id": "hexForamtEditor.dataInspectorView",
          "name": "%dataInspectorView%",
          "when": "hexEditor:showSidebarInspector"
        }
      ]
    },
    "menus": {
      "editor/title": [
        {
          "when": "activeEditor && config.hexformateditor.showOpenFileButton",
          "command": "formatEditor.openFile",
          "group": "navigation@1"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc --noEmit && node .esbuild.config.js",
    "lint": "eslint src --ext ts",
    "fmt": "prettier --write \"{src,media,shared}/**/*.{ts,tsx}\" && npm run lint -- --fix",
    "watch": "node .esbuild.config.js --watch",
    "test": "tsc --noEmit && node ./src/test/runTest.js"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.11.9",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vscode/test-electron": "^2.3.9",
    "chai": "^4.3.4",
    "esbuild": "^0.19.0",
    "esbuild-css-modules-plugin": "^3.1.0",
    "esbuild-plugin-svgr": "^2.1.0",
    "eslint": "^8.56.0",
    "mocha": "^9.1.3",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "@vscode/codicons": "0.0.27",
    "@vscode/extension-telemetry": "0.6.2",
    "cockatiel": "^3.1.2",
    "js-base64": "^3.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "recoil": "^0.7.7",
    "vscode-webview-tools": "^0.1.1"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "arrowParens": "avoid"
  }
}
